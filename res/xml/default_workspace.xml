<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.hct.scene.launcher" >

    <scene launcher:scene="default" />

    <wallpaper launcher:wallpaper="default_wallpaper" />
    <!-- Far-left screen [0] -->


    <!-- Left screen [1] -->
    <appwidget
        launcher:className="com.android.settings.widget.SettingsAppWidgetProvider"
        launcher:packageName="com.android.settings"
        launcher:screen="1"
        launcher:spanX="4"
        launcher:spanY="1"
        launcher:x="0"
        launcher:y="0" />

    <!-- 时钟 -->
    <favorite
        launcher:className="com.android.deskclock.DeskClock"
        launcher:packageName="com.android.deskclock"
        launcher:screen="1"
        launcher:x="0"
        launcher:y="3" />

    <!-- 日历 -->
    <favorite
        launcher:className="com.android.calendar.AllInOneActivity"
        launcher:packageName="com.android.calendar"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="3" />

    <!-- 电子邮件 -->
    <favorite
        launcher:className="com.android.email.activity.Welcome"
        launcher:packageName="com.android.email"
        launcher:screen="1"
        launcher:x="2"
        launcher:y="3" />

    <!-- 计算器 -->
    <favorite
        launcher:className="com.android.calculator2.Calculator"
        launcher:packageName="com.android.calculator2"
        launcher:screen="1"
        launcher:x="3"
        launcher:y="3" />

    <!-- screen 2 第三方网络应用 -->
    <!-- 数字时钟 for hct -->
    <appwidget
        launcher:className="com.hct.android.digtalclock.DigtalClockWidgetProvider"
        launcher:packageName="com.hct.android.digtalclock"
        launcher:screen="2"
        launcher:spanX="4"
        launcher:spanY="2"
        launcher:x="0"
        launcher:y="0" />

    <!-- 图库 -->
    <favorite
        launcher:className="com.android.gallery3d.app.Gallery"
        launcher:packageName="com.android.gallery3d"
        launcher:screen="2"
        launcher:x="0"
        launcher:y="3" />

    <!-- 相机 -->
    <favorite
        launcher:className="com.android.camera.Camera"
        launcher:packageName="com.android.camera"
        launcher:screen="2"
        launcher:x="1"
        launcher:y="3" />

    <!-- 记事本 -->
    <favorite
        launcher:className="com.mediatek.notebook.NotesList"
        launcher:packageName="com.mediatek.notebook"
        launcher:screen="2"
        launcher:x="2"
        launcher:y="3" />

    <!-- 设置 -->
    <favorite
        launcher:className="com.android.settings.Settings"
        launcher:packageName="com.android.settings"
        launcher:screen="2"
        launcher:x="3"
        launcher:y="3" />

    <!-- 3 -->
    <appwidget
        launcher:className="com.android.music.MediaAppWidgetProvider"
        launcher:packageName="com.android.music"
        launcher:screen="3"
        launcher:spanX="4"
        launcher:spanY="1"
        launcher:x="0"
        launcher:y="0" />

    <!-- 音乐 -->
    <favorite
        launcher:className="com.android.music.MusicBrowserActivity"
        launcher:packageName="com.android.music"
        launcher:screen="3"
        launcher:x="0"
        launcher:y="3" />

    <!-- 收音机 -->
    <favorite
        launcher:className="com.mediatek.FMRadio.FMRadioActivity"
        launcher:packageName="com.mediatek.FMRadio"
        launcher:screen="3"
        launcher:x="1"
        launcher:y="3" />

    <!-- 录音机 -->
    <favorite
        launcher:className="com.android.soundrecorder.SoundRecorder"
        launcher:packageName="com.android.soundrecorder"
        launcher:screen="3"
        launcher:x="2"
        launcher:y="3" />

    <!-- 文件管理 -->
    <favorite
        launcher:className="com.mediatek.filemanager.FileManagerOperationActivity"
        launcher:packageName="com.mediatek.filemanager"
        launcher:screen="3"
        launcher:x="3"
        launcher:y="3" />

    <!-- Hotseat (We use the screen as the position of the item in the hotseat) -->
    <favorite
        launcher:className="com.android.contacts.activities.DialtactsActivity"
        launcher:container="-101"
        launcher:packageName="com.android.contacts"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="0" />
    <favorite
        launcher:className="com.android.contacts.activities.PeopleActivity"
        launcher:container="-101"
        launcher:packageName="com.android.contacts"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="0" />
    <favorite
        launcher:className="com.android.mms.ui.BootActivity"
        launcher:container="-101"
        launcher:packageName="com.android.mms"
        launcher:screen="2"
        launcher:x="2"
        launcher:y="0" />
    <favorite
        launcher:className="com.android.browser.BrowserActivity"
        launcher:container="-101"
        launcher:packageName="com.android.browser"
        launcher:screen="3"
        launcher:x="3"
        launcher:y="0" />

</favorites>